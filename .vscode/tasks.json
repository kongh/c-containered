{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "CMake Build",
			"type": "shell",
			"command": "cmake --build ${workspaceFolder}/build",
			"problemMatcher": [
			  "$gcc"
			],
			"group": {
			  "kind": "build",
			  "isDefault": true
			}
		  },
		{
			"label": "CMake Clean",
			"type": "shell",
			"command": "cmake --build ${workspaceFolder}/build --target clean",
			"problemMatcher": []
		}
	]
  }